{
  "name": "redux-components",
  "version": "0.4.0",
  "description": "A component model for Redux state trees based on the React.js component model.",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib build es",
    "build:setup": "mkdirp build/intermediate && cp -R src/* build/intermediate",
    "build:coffee": "coffee --compile --output build/intermediate/ src/",
    "build:babel:commonjs": "mkdirp lib && cross-env BABEL_ENV=commonjs babel build/intermediate --out-dir lib",
    "build": "npm run build:setup && npm run build:coffee && npm run build:babel:commonjs",
    "test": "npm run test:mocha",
    "test:debug": "npm run build && npm run test:mocha:debug",
    "test:mocha": "mocha --recursive lib/_tests_/*.js",
    "test:mocha:debug": "node --inspect --debug-brk node_modules/.bin/_mocha --recursive lib/_tests_/*.js",
    "build:babel:istanbul": "mkdirp build/coverage && cross-env BABEL_ENV=istanbul babel build/intermediate --out-dir build/coverage",
    "coverage:build": "npm run build:babel:istanbul",
    "coverage:test": "node_modules/.bin/_mocha --recursive -- build/coverage/_tests_/*.test.js",
    "coverage:report": "nyc --reporter=lcov npm run coverage:test",
    "coverage": "npm run build:coffee && npm run coverage:build && npm run coverage:report",

    "build:babel:es": "cross-env BABEL_ENV=es babel build --out-dir es"
  },
  "keywords": [
    "redux",
    "flux",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wcjohnson/redux-components.git"
  },
  "author": "William C. Johnson <wcjohnson@oigroup.net> (https://github.com/wcjohnson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wcjohnson/redux-components/issues"
  },
  "homepage": "https://github.com/wcjohnson/redux-components",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.23.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-plugin-transform-es2015-object-super": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-lightscript": "^0.2.0",
    "chai": "^3.5.0",
    "coffeescript": "2.0.0-alpha1",
    "cross-env": "^3.1.3",
    "mocha": "^2.4.5",
    "redux": "^3.6.0",
    "redux-debug": "^1.0.3",
    "redux-freeze": "^0.1.5",
    "rimraf": "^2.6.1",
    "mkdirp": "^0.5.1"
  },
  "dependencies": {
    "nanotools": "^0.1.1",
    "observable-utils": "^0.1.1",
    "babel-runtime": "^6.23.0"
  }
}
